<div class="post__write-wrap box">
  <%= link_to new_post_path do %>
      <div class="post__write-icon"></div>
      <span class="post__write">포스트 작성하기</span>
  <% end %>
</div>

<div id="post__list">
  <% @posts.each do |post| %>
      <%= render partial: "post", object: post %>
  <% end %>
</div>

<%= paginate @posts %>

<%= link_to 'logout', destroy_user_session_path(), method: :delete %>